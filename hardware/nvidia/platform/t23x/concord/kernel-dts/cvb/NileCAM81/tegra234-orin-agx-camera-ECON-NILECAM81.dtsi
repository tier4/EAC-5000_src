/ {
		gpio@2200000 {
	
		camera-control-output-CAM0-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM0_PWDN 0>;
                        output-high;
                        label = "cam0-pwdn";
                };
		camera-control-output-CAM1-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM1_PWDN 0>;
                        output-high;
                        label = "cam1-pwdn";
                };
                camera-control-output-CAM2-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM2_PWDN 0>;
                        output-high;
                        label = "cam2-pwdn";
                };
                camera-control-output-CAM3-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM3_PWDN 0>;
                        output-high;
                        label = "cam3-pwdn";
                };
                camera-control-output-PWM-high {
                        status = "okay";
                        gpio-hog;
                        gpios = <CAM_PWM 0>;
                        output-high;
                        label = "cam-pwm";
                };
	};
	
